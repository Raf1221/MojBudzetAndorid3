<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.raf.mojbudzetandorid" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EkranGlowny"
            android:label="@string/title_activity_ekran_glowny" >
            <intent-filter>
                <action android:name="com.example.raf.mojbudzetandorid.EkranGlowny" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Przychody"
            android:label="@string/title_activity_przychody" >
            <intent-filter>
                <action android:name="com.example.raf.mojbudzetandorid.Przychody" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Wydatki"
            android:label="@string/title_activity_wydatki" >
            <intent-filter>
                <action android:name="com.example.raf.mojbudzetandorid.Wydatki" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Statystyki"
            android:label="@string/title_activity_statystyki" >
            <intent-filter>
                <action android:name="com.example.raf.mojbudzetandorid.Statystyki" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".my_dialog"
            android:label="@string/title_activity_my_dialog" >
        </activity>
    </application>

</manifest>
